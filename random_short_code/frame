from tkinter import *
from tkinter import messagebox
from tkinter import colorchooser
from tkinter import filedialog
window=Tk()
window.title('snakeeee')

logo=PhotoImage(file="c:\\Users\\sawa\\Desktop\\logo.png")
window.geometry('500x500')
window.iconphoto(True,logo)
window.config(background='#d46d66')

menubar=Menu(window)
fram=Frame(window)
fram.pack()
button1=Button(fram,text="w",width=3,height=3)
button1.pack(side=TOP)
button2=Button(fram,text="a",width=3,height=3)
button2.pack(side=LEFT)
button3=Button(fram,text="s",width=3,height=3)
button3.pack(side=LEFT)
button4=Button(fram,text="d",width=3,height=3)
button4.pack(side=LEFT)


window.mainloop()
