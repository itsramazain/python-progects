import random
from random import shuffle
import threading
import time
print('      welcome to a game of poker the rules are simple there are two players the first\n'
      '      plyer is called the dealer and its a pot here, you start with two cards facing up and \n'
      '      the dealer can show one of the two your goal is to get closer to 21 but not over than the\n'
      '      dealer you can choose to stay with your cards in hand or hit(which is to get more cards so the sum\n'
      '      increases) if you go over 21 you BUST, when you choose to stay the dealer uncovers the hidden card\n'
      '      of its under 17 the dealer has to hit if its sum is closer to 21 than you then you LOSE\n'
      '      if your closer to 21 then you win if the dealer sum is more than 21 the dealer BUSTS and you win\n'
      '        you get douple of what you wager if you win!! ')
suits = ('Hearts', 'Diamonds', 'Spades', 'Clubs')
ranks = ('Two', 'Three', 'Four', 'Five', 'Six', 'Seven', 'Eight', 'Nine', 'Ten', 'Jack', 'Queen', 'King', 'Ace')
values = {'Two':2, 'Three':3, 'Four':4, 'Five':5, 'Six':6, 'Seven':7, 'Eight':8,
            'Nine':9, 'Ten':10, 'Jack':10, 'Queen':10, 'King':10, 'Ace':11}
def choose():

    while True:

        try:
            choice=str(input('would you like to STAY of HIT')).lower()
        except TypeError:
            print('wrong input')
            continue
        except ValueError:
            continue
        if choice=='stay' or choice=='hit' :
            return choice
        else:
            print('sorry there is an issue please check the spilling ')
            continue


class Card():
    def __init__(self,rank,suit):
        self.rank=rank
        self.suit=suit
        self.value=values[self.rank]
    def __str__(self):
        return f'{self.rank} of {self.suit}'
class CardRanck():
    def __int__(self,rank):
        self.rank=rank
        self.value=values[self.rank]
class Deck():

    def __int__(self):
        self.all_cards=[]
        for suit in suits:
            for rank in ranks:
                self.all_cards.append(Card(rank,suit))
    def takefromdeck(self):
        self.all_cards.pop(0)
    def shuffle(self):
        shuffle(self.all_cards)
class Plyer():
    def __init__(self):
        self.playercards=[]
    def add(self):
        self.playercards.append()
def amount():
    while True:
        try:
            money=int(input('what balance do you wanna start with'))
            print('note you get the duple if you win')
        except TypeError:
            print('there is an issue please enter in digits not letters')
        except ValueError:
            continue
        return money

def choosevalue():
    while True:
        try:
            inn=int(input('you have an Ace which can add 1 or 11 to your sum please choose a value for the card '))
        except TypeError:
            print('there is an issue maybe you wrote it in letters no digits')
            continue
        except ValueError :
            continue
        if inn==1 or inn==11:
            return inn
            break
        else:
            print('you entered a wrong number')
