deck=[]

for rank in ranks :
    for suit in suits:
        deck.append(Card(rank,suit))
shuffle(deck)
total_com=0
total_plyer=0
class Bank():
    def __init__(self,balance):
        self.balnce=balance
    def win(self,money):
        self.balnce=self.balnce+2*money
    def lost(self,mony):
        self.balnce=self.balnce-mony
    def taketoplay(self,wage):
        self.balnce = self.balnce -wage

def askforbalance():
    print('if you win you get the your original balance plus duple wagered ')
    while True:
        try:
            balance=int(input('how much would you like to have a balance'))
        except:
            print('there is an issue please enter nuber is digits or reenter the number ')
        else:
            return balance
            break
account=Bank(askforbalance())
def askforwager():
    while True:
        try:
            wager=int(input('how much would you like to wager for this round'))
        except:
            print('there is an issue please enter nuber is digits or reenter the number')
            continue
        if wager<=account.balnce:
            account.taketoplay(wager)
            return wager
            break
        else:
            print('you dont have enough')
            continue
q1=askforwager()
